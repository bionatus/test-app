{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-grpc": "^1.36",
        "ext-imagick": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "arubacao/tld-checker": "^1.2",
        "bordoni/phpass": "^0.3.5",
        "coduo/php-humanizer": "^4.0",
        "dillingham/nova-ajax-select": "^1.0",
        "dillingham/nova-button": "^1.0",
        "doctrine/dbal": "^2.9",
        "ebess/advanced-nova-media-library": "^3.6",
        "epartment/nova-dependency-container": "^1.3",
        "fideloper/proxy": "^4.0",
        "fourstacks/nova-checkboxes": "^0.1.7",
        "fruitcake/laravel-cors": "^2.0",
        "genealabs/nova-map-marker-field": "^0.3.1",
        "gmostafa/php-graphql-client": "^1.13",
        "google/cloud-firestore": "^1.19",
        "hubspot/api-client": "^1.3",
        "kreait/laravel-firebase": "3.4.0",
        "laravel-notification-channels/fcm": "^2.2",
        "laravel-notification-channels/twilio": "^3.2",
        "laravel/cashier": "^12.4",
        "laravel/framework": "^8.0",
        "laravel/nova": "*",
        "laravel/passport": "^10.0",
        "laravel/spark-aurelius": "*@dev",
        "laravel/telescope": "^4.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.4",
        "league/flysystem-aws-s3-v3": "~1.0",
        "maatwebsite/excel": "^3.1",
        "malhal/laravel-geographical": "^1.0",
        "menarasolutions/geographer": "^0.3.12",
        "menarasolutions/geographer-es": "^0.1.0",
        "metrixinfo/nova-iframe": "^1.0",
        "mpyw/laravel-local-class-scope": "^2.0",
        "nutshellcrm/nutshell-api": "dev-master",
        "predis/predis": "^2.1",
        "pubnub/pubnub": "^5.0",
        "sentry/sentry-laravel": "^2.8",
        "skagarwal/google-places-api": "^1.7",
        "spatie/geocoder": "^3.11",
        "spatie/laravel-activitylog": "^3.17",
        "spatie/laravel-backup": "^6.7",
        "spatie/laravel-medialibrary": "^9.0.0",
        "spatie/laravel-permission": "~4.0",
        "spatie/laravel-queueable-action": "^2.12",
        "spatie/laravel-sluggable": "^2.6",
        "symfony/lock": "^5.0",
        "titasgailius/search-relations": "^2.0",
        "twilio/sdk": "^6.22",
        "tymon/jwt-auth": "1.0.*"
    },
    "require-dev": {
        "ext-simplexml": "*",
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.10",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fakerphp/faker": "^1.20",
        "jasonmccreary/laravel-test-assertions": "^2.0",
        "joshgaber/novaunit": "^2.2",
        "laravel/homestead": "^11",
        "mockery/mockery": "^1.0",
        "mohammedmanssour/form-request-tester": "^1.1",
        "nunomaduro/collision": "^5.0",
        "opis/json-schema": "^2.0",
        "phpunit/phpunit": "^9.0",
        "symfony/dom-crawler": "^5.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "symfony/thanks": false
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeds/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "path",
            "url": "./spark"
        },
        {
            "type": "path",
            "url": "./nova"
        },
        {
            "type": "vcs",
            "url": "https://github.com/nutshellcrm/nutshell-api-php"
        },
        {
            "type": "vcs",
            "url": "https://github.com/deqngeorgiev/php-library2"
        }
    ]
}
